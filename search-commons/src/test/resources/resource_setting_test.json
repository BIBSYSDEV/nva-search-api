{
  "analysis": {
    "analyzer": {
      "default": {
        "tokenizer": "lowercase",
        "filter": [
          "my_ascii_folding"
        ]
      }
    },
    "filter": {
      "standard_nva_filter": {
        "type": "asciifolding",
        "preserve_original": true
      }
    },
    "normalizer": {
      "custom_normalizer": {
        "type": "custom",
        "filter": [
          "my_ascii_folding",
          "lowercase",
          "scandinavian_normalization "
        ]
      }
    }
  }
}