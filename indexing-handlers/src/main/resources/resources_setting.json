{
  "analysis": {
    "analyzer": {
      "default": {
        "tokenizer": "lowercase",
        "filter": [
          "my_ascii_folding"
        ]
      }
    },
    "filter": {
      "my_ascii_folding": {
        "type": "asciifolding",
        "preserve_original": true
      }
    },
    "normalizer": {
      "custom_normalizer": {
        "type": "custom",
        "filter": [
          "asciifolding",
          "lowercase",
          "scandinavian_folding"
        ]
      }
    }
  }
}